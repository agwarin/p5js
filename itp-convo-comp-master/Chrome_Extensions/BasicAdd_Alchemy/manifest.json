{
  "manifest_version": 2,

  "name": "Sentiment Add",
  "description": "This extension demonstrates a content script with AlchemyAPI.",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": [
      "jquery.js",
      "content_script.js"
    ],
    "css": [
      "style.css"
    ]
  }]
}
